    /**
     * Index method
     *
     * @return \Cake\Http\Response|null|void Renders view
     */
    public function index()
    {
        $query = $this->{{ currentModelName }}->find();
{% set belongsTo = Bake.aliasExtractor(modelObj, 'BelongsTo') %}
{% if belongsTo %}
        $query->contain([{{ Bake.stringifyList(belongsTo, {'indent': false})|raw }}]);
       
{% endif %}
        $options['order'] = ['{{ currentModelName }}.id' => 'DESC'];
        $options['limit'] = \Cake\Core\Configure::read('Setting.ADMIN_PAGE_LIMIT');
        $this->paginate = $options;
        ${{ pluralName }} = $this->paginate($query);
        $this->set(compact('{{ pluralName }}'));
    }
