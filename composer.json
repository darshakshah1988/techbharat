{
    "name": "cakephp/app",
    "description": "CakePHP skeleton app",
    "homepage": "https://cakephp.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "admad/cakephp-glide": "4.0.0-RC",
        "admad/cakephp-sequence": "^3.0",
        "cakedc/users": "^9.0",
        "cakephp/authentication": "^2.3",
        "cakephp/authorization": "^2.0",
        "cakephp/cakephp": "4.1.*",
        "cakephp/elastic-search": "^3.2",
        "cakephp/migrations": "^3.0",
        "cakephp/plugin-installer": "^1.2",
        "dereuromark/cakephp-queue": "^5.0",
        "dereuromark/cakephp-tools": "^2.0",
        "friendsofcake/cakepdf": "^4.0",
        "friendsofcake/search": "^6.0",
        "josegonzalez/cakephp-upload": "5.0.0-RC",
        "league/oauth1-client": "@stable",
        "league/oauth2-facebook": "@stable",
        "league/oauth2-google": "@stable",
        "lorenzo/audit-stash": "^3.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "mpdf/mpdf": "^8.0",
        "muffin/slug": "^2.0",
        "razorpay/razorpay": "2.*",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "cakephp/bake": "^2.0.3",
        "cakephp/cakephp-codesniffer": "~4.2.0",
        "cakephp/debug_kit": "^4.1",
        "josegonzalez/dotenv": "^3.2",
        "phpunit/phpunit": "~8.5.0",
        "psy/psysh": "@stable"
    },
    "suggest": {
        "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
        "dereuromark/cakephp-ide-helper": "After baking your code, this keeps your annotations in sync with the code evolving from there on for maximum IDE and PHPStan/Psalm compatibility.",
        "phpstan/phpstan": "PHPStan focuses on finding errors in your code without actually running it. It catches whole classes of bugs even before you write tests for the code."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "AdminUserManager\\": "./plugins/AdminUserManager/src/",
            "BackEnd\\": "./plugins/BackEnd/src/",
            "CustomTheme\\": "./plugins/CustomTheme/src/",
            "CommonFront\\": "./plugins/CommonFront/src/",
            "CommonTheme\\": "./plugins/CommonTheme/src/",
            "Cms\\": "./plugins/Cms/src/",
            "Locations\\": "./plugins/Locations/src/",
            "Listings\\": "./plugins/Listings/src/",
            "Media\\": "./plugins/Media/src/",
            "Courses\\": "./plugins/Courses/src/",
            "EmailManager\\": "./plugins/EmailManager/src/",
            "Settings\\": "./plugins/Settings/src/",
            "News\\": "./plugins/News/src/",
            "Events\\": "./plugins/Events/src/",
            "Enquiry\\": "./plugins/Enquiry/src/",
            "Announcement\\": "./plugins/Announcement/src/",
            "Services\\": "./plugins/Services/src/",
            "Testimonials\\": "./plugins/Testimonials/src/",
            "TeachingTheme\\": "plugins/TeachingTheme/src/",
            "UserManager\\": "plugins/UserManager/src/",
            "Language\\": "plugins/Language/src/",
            "Orders\\": "plugins/Orders/src/",
            "Referrals\\": "plugins/Referrals/src/",
            "Transactions\\": "plugins/Transactions/src/",
            "Reviews\\": "plugins/Reviews/src/",
            "Promotions\\": "plugins/Promotions/src/",
            "Sessions\\": "plugins/Sessions/src/",
            "MicroSessions\\": "plugins/MicroSessions/src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "AdminUserManager\\Test\\": "./plugins/AdminUserManager/tests/",
            "BackEnd\\Test\\": "./plugins/BackEnd/tests/",
            "CustomTheme\\Test\\": "./plugins/CustomTheme/tests/",
            "CommonFront\\Test\\": "./plugins/CommonFront/tests/",
            "CommonTheme\\Test\\": "./plugins/CommonTheme/tests/",
            "Cms\\Test\\": "./plugins/Cms/tests/",
            "Locations\\Test\\": "./plugins/Locations/tests/",
            "Listings\\Test\\": "./plugins/Listings/tests/",
            "Media\\Test\\": "./plugins/Media/tests/",
            "Courses\\Test\\": "./plugins/Courses/tests/",
            "EmailManager\\Test\\": "./plugins/EmailManager/tests/",
            "Settings\\Test\\": "./plugins/Settings/tests/",
            "LanuageManager\\Test\\": "./plugins/LanuageManager/tests/",
            "News\\Test\\": "./plugins/News/tests/",
            "Events\\Test\\": "./plugins/Events/tests/",
            "Enquiry\\Test\\": "./plugins/Enquiry/tests/",
            "Announcement\\Test\\": "./plugins/Announcement/tests/",
            "Services\\Test\\": "./plugins/Services/tests/",
            "Testimonials\\Test\\": "./plugins/Testimonials/tests/",
            "TeachingTheme\\Test\\": "plugins/TeachingTheme/tests/",
            "UserManager\\Test\\": "plugins/UserManager/tests/",
            "Language\\Test\\": "plugins/Language/tests/",
            "Orders\\Test\\": "plugins/Orders/tests/",
            "Referrals\\Test\\": "plugins/Referrals/tests/",
            "Transactions\\Test\\": "plugins/Transactions/tests/",
            "Reviews\\Test\\": "plugins/Reviews/tests/",
            "Promotions\\Test\\": "plugins/Promotions/tests/",
            "Sessions\\Test\\": "plugins/Sessions/tests/",
            "MicroSessions\\Test\\": "plugins/MicroSessions/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-create-project-cmd": "App\\Console\\Installer::postInstall",
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs --colors -p  src/ tests/",
        "cs-fix": "phpcbf --colors -p src/ tests/",
        "stan": "phpstan analyse src/",
        "test": "phpunit --colors=always"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
